{
	"name": "Broadmask Facebook Decrypt",
	"version": "0.1",
	"description": "Bringing Pairing based Cryptography to Facebook",
	"icons": { "128": "icon.png" },
	"background_page": "background.html",
	"option_page": "options.html",
	"browser_action": {
	    "default_icon": "icon.png"
	  },
	  "permissions": [
		"tabs",
		"unlimitedStorage",
	    "https://*.google.com/*",
	    "https://*.facebook.com/*",
			"http://gpg-keyserver.de/pks/lookup/*"
	  ]
}
